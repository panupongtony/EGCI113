#include <stdio.h>
#include <stdlib.h>
#define SIZE 10
int main()
{
   int a[SIZE];
   int sum;
   for(int i = 0;i<SIZE;i++)
   {
       printf("Input number of student%d: ",i+1);
       scanf("%d",&a[i]);
       sum+=a[i];
   }
   float average = (float)sum/SIZE;
   printf("The average is %.2f\n",average);
  for(int i = 0;i<SIZE;i++)
  {
      if(average>a[i]) printf("Student %d has less score than average\n",i+1);
      else if(average<a[i]) printf("Student %d has more score than average\n",i+1);
      else if(average==a[i])printf("Student %d has score equal to average\n",i+1);
  }
    return 0;
}
