#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b;
    printf("Insert 2 number\n");
    scanf("%d,%d",&a,&b);
    int gcd;
    int i = 1;
    while(i<=a&&i<=b)
        {
        if(a%i==0&&b%i==0){
            gcd = i;
        }i++;
    }
    printf("GCD is %d",gcd);

    return 0;
}
